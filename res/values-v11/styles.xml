<resources>

    <!--
        Base application theme for API 11+. This theme completely replaces
        AppBaseTheme from res/values/styles.xml on API 11+ devices.    -->
    <style name="AppTheme" parent="android:Theme.Holo.Light">
        <item name="refresh_icon">@drawable/navigation_refresh_light</item>
        <item name="user_icon">@drawable/social_person_light</item>
    </style>

    <style name="AppTheme_Dark" parent="android:Theme.Holo">
        <item name="refresh_icon">@drawable/navigation_refresh_dark</item>
        <item name="user_icon">@drawable/social_person_dark</item>
    </style>

</resources>