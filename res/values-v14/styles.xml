<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!--
        Base application theme for API 14+. This theme completely replaces
        AppBaseTheme from BOTH res/values/styles.xml and
        res/values-v11/styles.xml on API 14+ devices.
    -->
    <style name="AppTheme" parent="android:Theme.Holo.Light.DarkActionBar">
        <item name="refresh_icon">@drawable/navigation_refresh_dark</item>
        <item name="user_icon">@drawable/social_person_light</item>
    </style>
    
    <style name="AppTheme_Dark" parent="android:Theme.Holo">
        <item name="refresh_icon">@drawable/navigation_refresh_dark</item>
        <item name="user_icon">@drawable/social_person_dark</item>
    </style>

</resources>
